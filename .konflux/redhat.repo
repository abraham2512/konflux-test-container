[rhel-8-for-$basearch-appstream-rpms]
name = Red Hat Enterprise Linux 8 for $basearch - AppStream (RPMs)
baseurl = https://cdn.redhat.com/content/dist/rhel8/8/$basearch/appstream/os
enabled = 1
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-release
sslverify = 1
sslcacert = /etc/rhsm/ca/redhat-uep.pem
sslclientkey = /etc/pki/entitlement/5832948408471545157-key.pem
sslclientcert = /etc/pki/entitlement/5832948408471545157.pem
sslverifystatus = 1
metadata_expire = 86400
enabled_metadata = 1

